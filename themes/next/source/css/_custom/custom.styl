        background-repeat: no-repeat;
        background-attachment:fixed;
        background-position:30% 70%;
    }

    .main-inner { 
        margin-top: 50px;
        padding: 50px 50px 50px 50px;
        background: #fff;
        min-height: 500px;
    }
}